        android:label="Android"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:label">
        <activity android:name=".firstpage.MainActivity" />
        <activity android:name=".firstpage.signuppage" />
        <activity android:name=".firstpage.login" />
        <activity android:name=".firstpage.help" />
        <activity android:name=".firstpage.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".expandableView.expandable_listview" />
        <activity android:name=".listviewpack.customlistmain" />
        <activity android:name=".listviewpack.afterclickcustom" />
        <activity android:name=".firstpage.loginout" />
