            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Retrofitwithgirdandrect.imagebigceleb"
            android:theme="@style/CustomTheme" />
        <activity android:name=".basic_animation" />
        <activity android:name=".sqlite_db" />
        <activity android:name=".AddCountryActivity" />
        <activity android:name=".ModifycountryActivity" />
    </application>

</manifest>